{
  "manifest_version": 3,
  "name": "TestingMate ID Generator",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon/testingMate-icon-16.png",
    "32": "icon/testingMate-icon-32.png",
    "48": "icon/testingMate-icon-48.png",
    "128": "icon/testingMate-icon-128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "declarativeContent"
  ],
  "host_permissions": ["<all_urls>"]
}
